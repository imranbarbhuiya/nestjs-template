{
	"$schema": "https://json.schemastore.org/swcrc",
	"sourceMaps": true,
	"module": {
		"type": "commonjs",
		"strict": true
	},
	// "exclude": ["^.+\\.test\\.ts$"],
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": true
		},
		"target": "es2022",
		"keepClassNames": true,
		"transform": {
			"legacyDecorator": true,
			"decoratorMetadata": true
		},
		"externalHelpers": true
	}
}
