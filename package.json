{
	"name": "@roti/web-api",
	"description": "The backend api for roti.xyz",
	"version": "0.0.0",
	"scripts": {
		"prebuild": "rm -r dist",
		"dev": "nest start --watch",
		"build": "nest build",
		"start": "node --enable-source-maps dist/main.js",
		"lint": "TIMING=1 eslint \"{src,test}/**/*.ts\" --fix",
		"format": "prettier --write --cache .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
		"test:e2e": "vitest run --config ./vitest.config.e2e.ts",
		"typecheck": "tsc -p tsconfig.eslint.json"
	},
	"dependencies": {
		"@imranbarbhuiya/duration": "^5.1.8",
		"@nestjs/common": "^10.1.3",
		"@nestjs/config": "^3.0.0",
		"@nestjs/core": "^10.1.3",
		"@nestjs/mongoose": "^10.0.1",
		"@nestjs/passport": "^10.0.0",
		"@nestjs/platform-express": "^10.1.3",
		"@nestjs/serve-static": "^4.0.0",
		"@nestjs/swagger": "^7.1.4",
		"@oauth-everything/passport-discord": "^1.0.2",
		"@swc/helpers": "^0.5.1",
		"@top-gg/sdk": "^3.1.5",
		"connect-mongo": "^5.0.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"express-session": "^1.17.3",
		"mongoose": "^7.4.1",
		"passport": "^0.6.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"tslib": "^2.6.1",
		"undici": "^5.22.1",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@darraghor/eslint-plugin-nestjs-typed": "^4.0.2",
		"@nestjs/cli": "^10.1.11",
		"@nestjs/schematics": "^10.0.1",
		"@nestjs/testing": "^10.1.3",
		"@swc/cli": "^0.1.62",
		"@swc/core": "^1.3.72",
		"@types/express": "^4.17.17",
		"@types/express-session": "^1.17.7",
		"@types/node": "^20.4.5",
		"@types/passport": "^1.0.12",
		"@vitest/coverage-v8": "^0.33.0",
		"eslint": "^8.46.0",
		"eslint-config-mahir": "^0.0.31",
		"prettier": "^3.0.0",
		"typescript": "^5.1.6",
		"unplugin-swc": "^1.3.2",
		"vitest": "^0.33.0"
	},
	"packageManager": "yarn@3.6.1"
}
