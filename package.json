{
	"name": "@roti/web-api",
	"description": "The backend api for roti.xyz",
	"version": "0.0.0",
	"scripts": {
		"prebuild": "rm -r dist",
		"dev": "nest start --watch",
		"build": "nest build",
		"start": "node --enable-source-maps dist/main.js",
		"lint": "TIMING=1 eslint \"{src,test}/**/*.ts\" --fix",
		"format": "prettier --write --cache .",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
		"test:e2e": "vitest run --config ./vitest.config.e2e.ts",
		"typecheck": "tsc -p tsconfig.eslint.json"
	},
	"dependencies": {
		"@imranbarbhuiya/duration": "^5.1.8",
		"@nestjs/common": "^10.4.13",
		"@nestjs/config": "^3.3.0",
		"@nestjs/core": "^10.4.13",
		"@nestjs/mongoose": "^10.1.0",
		"@nestjs/passport": "^10.0.3",
		"@nestjs/platform-express": "^10.4.13",
		"@nestjs/serve-static": "^4.0.2",
		"@nestjs/swagger": "^8.1.0",
		"@oauth-everything/passport-discord": "^1.0.2",
		"@swc/helpers": "^0.5.15",
		"@top-gg/sdk": "^3.1.6",
		"connect-mongo": "^5.1.0",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"express-session": "^1.18.1",
		"mongoose": "^8.8.4",
		"passport": "^0.7.0",
		"reflect-metadata": "^0.2.1",
		"rxjs": "^7.8.1",
		"tslib": "^2.8.1",
		"undici": "^7.1.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@darraghor/eslint-plugin-nestjs-typed": "^5.0.29",
		"@nestjs/cli": "^10.4.8",
		"@nestjs/schematics": "^10.2.3",
		"@nestjs/testing": "^10.4.13",
		"@swc/cli": "^0.5.2",
		"@swc/core": "^1.10.0",
		"@types/express": "^4.17.21",
		"@types/express-session": "^1.18.1",
		"@types/node": "^22.10.1",
		"@types/passport": "^1.0.17",
		"@vitest/coverage-v8": "^2.1.8",
		"eslint": "^9.20.1",
		"eslint-config-mahir": "^0.0.46",
		"prettier": "^3.4.2",
		"typescript": "^5.7.2",
		"unplugin-swc": "^1.5.1",
		"vitest": "^2.1.8"
	},
	"packageManager": "yarn@4.5.3"
}
